page{
  background: #fff;
}
.more-list{
  border-top: 1px solid #e6e6e6;
}
.more-list-tr{
  padding:40rpx 20rpx;
  border-bottom: 1px solid #e6e6e6;
  display: flex;
  flex-direction: row;
  align-items: center;
  position: relative;
}
.index-icon{
  width: 66rpx;
  height: 66rpx;
  margin-right: 30rpx;
}
.more-list-tr text{
  font-size: 30rpx;
  color: #333;
}
.triangle-right{
  width: 12rpx;
  height: 22rpx;
  position: absolute;
  right: 20rpx;
  top:50%;
  margin-top: -11rpx;
}